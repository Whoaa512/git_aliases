# Short Git aliases
  alias gi='git init'
  alias gir='git init; touch README.md;gaa;'
  alias gii='git init; touch .gitignore;gaa;'
  alias giir='git init; touch .gitignore README.md;gaa;'
  alias gs='git status'
  alias gst='git stash'
  alias gsta='git stash apply'
  alias gstp='git stash pop'
  alias ga='git add'
  alias gaa='git add .;gs'
  alias grm='git rm'
  alias grmf='git rm -f'
  alias grmrf='git rm -rf'
  alias gb='git branch'
  alias gcl='git clone'
  alias gc='git commit'
  alias gco='git checkout'
  alias gcob='git checkout -b'
  alias gd='git diff'
  alias gdh='git diff head'
  alias gf='git fetch'
  alias gl='git log --oneline'
  alias gpl='git pull'
  alias gplr='git pull --rebase'
  alias gplro='git pull --rebase origin'
  alias gplrom='git pull --rebase origin master'
  alias gplrogh='git pull --rebase origin gh-pages'
  alias gplrod='git pull --rebase origin dev'
  alias gplo="git pull origin"
  alias gplom="git pull origin master"
  alias gplod="git pull origin dev"
  alias gp='git push'
  alias gpo='git push origin'
  alias gpfom='git push -f origin master'
  alias gpom='git push origin master'
  alias gpogh='git push origin gh-pages'
  alias gpod='git push origin dev'
  alias grb='git rebase'
  alias grbc='git rebase --continue'
  alias gr='git remote'
  alias gra='git remote add'
  alias grao='git remote add origin'
  alias grr='git remote rm'
  alias grv='git remote -v'
  alias gba='git branch -a'
  alias del='git branch -d'
